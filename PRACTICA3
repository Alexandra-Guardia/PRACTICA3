from flask import Flask, render_template
app=Flask (__name__)
@app.route("/")
def index():
    return "Hola bienvenido"

@app.route("/Saludo/<string:name>/<int:age>")
def saludo(name,age):
    return f'hola {name} tienes {age} a√±os' 
@app.route("/render")  
def render():
    return render_template("PRUEBA.HTML")
